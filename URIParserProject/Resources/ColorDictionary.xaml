<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="ColorBlue">#009DDA</Color>
    <Color x:Key="ColorBlue1">#70bfff</Color>
    <Color x:Key="ColorRed">#CC3333</Color>
    <Color x:Key="ColorRed1">#E8A4A4</Color>
    <Color x:Key="ColorGreen">#74BA11</Color>
    <Color x:Key="ColorGreen1">#E8FBCE</Color>
    <Color x:Key="ColorGray1">#F8F8F8</Color>
    <Color x:Key="ColorGray2">#F2F2F2</Color>
    <Color x:Key="ColorGray3">#E4E4E4</Color>
    <Color x:Key="ColorGray4">#C9C9C9</Color>
    <Color x:Key="ColorGray5">#666666</Color>
    <SolidColorBrush x:Key="Blue" Color="{StaticResource ColorBlue}"/>
    <SolidColorBrush x:Key="Blue1" Color="{StaticResource ColorBlue1}"/>
    <SolidColorBrush x:Key="Red" Color="{StaticResource ColorRed}"/>
    <SolidColorBrush x:Key="Red1" Color="{StaticResource ColorRed1}"/>
    <SolidColorBrush x:Key="Green" Color="{StaticResource ColorGreen}"/>
    <SolidColorBrush x:Key="Green1" Color="{StaticResource ColorGreen1}"/>
    <SolidColorBrush x:Key="Gray1" Color="{StaticResource ColorGray1}"/>
    <SolidColorBrush x:Key="Gray2" Color="{StaticResource ColorGray2}"/>
    <SolidColorBrush x:Key="Gray3" Color="{StaticResource ColorGray3}"/>
    <SolidColorBrush x:Key="Gray4" Color="{StaticResource ColorGray4}"/>
    <SolidColorBrush x:Key="Gray5" Color="{StaticResource ColorGray5}"/>
</ResourceDictionary>